{
  "name": "fortytwo-studio/webfaction-php",
  "description": "PHP wrapper for the Webfaction XML-RPC API",
  "license": "MIT",
  "authors": [
    {
      "name": "Billy Fagan",
      "email": "billy@fortytwo.studio"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/fortytwostudio/webfaction-php"
    }
  ],
  "require": {
    "phpxmlrpc/phpxmlrpc": "~4"
  },
  "require-dev": {
    "phpunit/phpunit": "~7",
    "vlucas/phpdotenv": "^2.4"
  },
  "autoload": {
    "psr-4": {
      "FortyTwoStudio\\WebFactionPHP\\" : "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "FortyTwoStudio\\WebFactionPHP\\" : ["src/", "tests/"]
    }
  }
}
